    {
      for(int j = 0; j<array[0].length;j++)
          {
        if(array[i][j] == 1)
        {
          System.out.print(":");
        }
        else if(array[i][j] == 4)
        {
        System.out.print("*");
        }
        System.out.print(" ");
      }
        System.out.print("\n");
          }
          }
  public static boolean solve(int [][] array, int r, int c)
  {
    if(r > array.length || c > array[0].length|| c <0 || r<0)
    {
      return false;
    }
    if(array[r][c] == 1)
    {
      return false;
    }
    if(array[r][c] == 3)
    {
      return true; 
    }
    if(array[r][c] == 4 || array[r][c] == 5)
    {
      return false;
    }
    
    
    array[r][c] = 4;
    if(solve(array, r-1, c))
    {
      return true;
    }
      if(solve(array, r, c-1))
    {
      return true;
    }
      if(solve(array, r+1, c))
    {
      return true;
    }
      if(solve(array, r, c+1))
    {
      return true;
    }
    array[r][c] = 5;
    return false;
  }
  }