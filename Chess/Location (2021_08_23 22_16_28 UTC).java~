//Location

public class Location
{
  private int row, column;
  private String loc;
  public Location(int r,int c)
  {
    row = r;
    column = c;
  }
  public Location(String loc)
  {
    loc = loc.toUpperCase();
    char first = loc.charAt(0);
    int number = (int)first;
    if(loc.length() >2||number<65||number>72||loc.charAt(1)>=1&&loc.charAt(1)<=8)
    {
      throw new IllegalArgumentException("F");
    }
    else 
    {
      row =  8 - (loc.charAt(1)-'0');
      column = loc.charAt(0)-'A';
      Location n = new Location(row,column);
    }
  }
  public void setRow(int r)
  {
    row = r;
  }
  public int getRow()
  {
    return row;
  }
  public void setCol(int c)
  {
    column = c;
  }
  public int getCol()
  {
    return column;
  }
  public String toString()
  {
    String results = "Row: "+ row + "\nColumn: " + column;
    return results;
  }
  public boolean equals(Location n)
  {
    return (row==n.getRow()&&column==n.getCol()); 
  }
}