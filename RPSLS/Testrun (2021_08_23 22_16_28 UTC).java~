          {
            throwList.add(2);
          }
          if(list.get(f)==4)
          {
            throwList.add(3);
          }
        }
      }

      if(random==3)
      {
        for(int f = 0; f<list.size();f++)
        {
          if(list.get(f)==0)
          {
            throwList.add(1);
          }
          if(list.get(f)==1)
          {
            throwList.add(2);
          }
          if(list.get(f)==2)
          {
            throwList.add(4);
          }
          if(list.get(f)==3)
          {
            throwList.add(2);
          }
          if(list.get(f)==4)
          {
            throwList.add(3);
          }
        }
      }
      if(random==4)
      {
        for(int f = 0; f<list.size();f++)
        {
          if(list.get(f)==0)
          {
            throwList.add(4);
          }
          if(list.get(f)==1)
          {
            throwList.add(3);
          }
          if(list.get(f)==2)
          {
            throwList.add(4);
          }
          if(list.get(f)==3)
          {
            throwList.add(0);
          }
          if(list.get(f)==4)
          {
            throwList.add(1);
          }
        }
      }
      for (int u = 0; u<list.size();u++)
      {
        if (list.get(u) == first)
        {
          difference = u;
        }
      }
      if(list.size() == 23)
      {
        list.clear();
      }
      turn = true;
    }
