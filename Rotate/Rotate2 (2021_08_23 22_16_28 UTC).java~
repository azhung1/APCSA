import java.util.Scanner;
public class Rotate2
{ 
  public static void main(String[] args) 
  {
    int row, column;
    String h = "";
    Scanner scan = new Scanner(System.in);
    System.out.print("What is the row of your matrix?");
    row = scan.nextInt();
    System.out.print("What is the column of your matrix?");
    column = scan.nextInt();
    int arr[][] = new int [row][column]; 
    for (int b = 0;b<row*column;b++)
    {
      int row1 = 0;
      int column1 = 0;
      System.out.print( "What is your desired input?");
      int input = scan.nextInt();
      arr[row1][column1] = input;
      h+= Integer.toString(input);
      row1++;
      column1++;
    }
//    System.out.print(rotate(arr, 90));
